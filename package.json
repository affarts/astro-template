{
  "name": "astro-template",
  "type": "module",
  "version": "0.0.1",
  "browserslist": [
    "cover 99.5%"
  ],
  "astro": {
    "bundlerOptions": {
      "css": {
        "minify": true,
        "extract": {
          "filename": "[name].css"
        }
      }
    }
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev --host",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "test": "vitest",
    "check": "astro check",
    "check:ts": "tsc --noEmit",
    "prepare": "husky",
    "lint:prettier": "prettier -w **/*.{js,jsx,ts,tsx,md,mdx,astro} --config .prettierrc.cjs",
    "lint:eslint": "eslint src/**/*.{js,ts,jsx,tsx,astro} --fix",
    "lint:stylelint": "stylelint **/*.scss",
    "lint:stylelint:fix": "stylelint **/*.scss --fix",
    "lint": "npm run lint:prettier & npm run lint:eslint & npm run lint:stylelint",
    "gen:robots": "node ./util/create-robots-file.mjs",
    "images": "node ./util/images-optimize.mjs"
  },
  "dependencies": {
    "@fluejs/noscroll": "^1.0.0",
    "astro": "^5.14.1",
    "astro-icon": "^1.1.0",
    "resize-observer-polyfill": "^1.5.1",
    "simplebar": "^6.2.7",
    "swiper": "^11.0.6"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/ts-plugin": "^1.10.4",
    "@stylistic/stylelint-plugin": "^3.1.2",
    "@tweakpane/core": "^2.0.4",
    "@types/jsdom": "^21.1.7",
    "@types/swiper": "^6.0.0",
    "@typescript-eslint/parser": "^6.7.2",
    "autoprefixer": "^10.4.20",
    "chalk": "^5.3.0",
    "cssnano": "^7.0.6",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-astro": "^0.29.0",
    "fast-glob": "3.3.3",
    "glob": "^10.3.12",
    "husky": "^9.0.11",
    "inquirer": "^9.2.17",
    "inquirer-file-tree-selection-prompt": "^2.0.5",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.47",
    "postcss-combine-duplicated-selectors": "^10.0.3",
    "postcss-combine-media-query": "^1.0.1",
    "postcss-reporter": "^7.1.0",
    "postcss-scss": "^4.0.9",
    "postcss-sort-media-queries": "^5.2.0",
    "postcss-url": "^10.1.3",
    "prettier": "^3.1.1",
    "prettier-plugin-astro": "^0.12.3",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.93.2",
    "stylelint-scss": "^6.0.0",
    "typescript": "^5.3.3",
    "vitest": "^2.1.1"
  }
}
